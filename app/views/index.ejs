<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <link href='//fonts.googleapis.com/css?family=Roboto:400,300,700' rel='stylesheet' type='text/css'>
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
</head>
<body>
    <div class="content">
        <h2 class="title-bar">Node</h2>
        <section class="chat">
            <div class="users">
                <ul class="user-list"></ul>
            </div>
            <div class="messages">
                <div class="user-messages"></div>
                <form class="form-new-msg">
                    <input type="text" required class="form-text" >
                    <input type="button" value="Send" class="form-submit">
                </form>
            </div>
        </section>
    </div>

    <script src="//code.jquery.com/jquery.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/html_parser/html_parser.js"></script>
    <script src="/js/app.js"></script>
</body>
</html>


